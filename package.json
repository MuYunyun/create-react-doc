{
  "name": "create-react-doc",
  "version": "0.0.6",
  "description": "Fast static generated site. Just write markdown file.",
  "homepage": "",
  "bin": {
    "react-doc": "bin/react-doc.js"
  },
  "scripts": {
    "release": "release-it --src.tagName='v%s'",
    "start": "node bin/react-doc.js -d templates/default/home,templates/default/introduce,templates/default/faq,templates/default/about,templates/default/github",
    "build": "node bin/react-doc.js -d templates/default/home,templates/default/introduce,templates/default/faq,templates/default/about,templates/default/github --build",
    "deploy": "node bin/react-doc.js --publish git@github.com:react-doc/react-doc.github.io.git --branch master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MuYunyun/create-react-doc"
  },
  "keywords": [
    "react",
    "create-react-doc",
    "blog",
    "markdown"
  ],
  "author": "muyunyun",
  "license": "MIT",
