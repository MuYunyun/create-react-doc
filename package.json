    "build": "node packages/create-react-doc/index.js build",
    "deploy": "node packages/create-react-doc/index.js deploy",
    "release": "lerna publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MuYunyun/create-react-doc"
  },
  "keywords": [
